{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-console":"off",
      "no-alert":"off",
      "no-undef": "off",
      "import/no-unresolved":"off",
      "no-empty-function": "off",
      "no-unused-vars": "off",
      "no-useless-constructor": "off",
      "linebreak-style":"off",
      "import/prefer-default-export":"off",
      "class-methods-use-this":"off"
    }
}
